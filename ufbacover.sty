% cover custom command
\newcommand{\newsketchedcover}[1]{
    %% cover art with latex
    \afterpage{
        \selectcolormodel{cmyk}
        \thispagestyle{empty}
        \newgeometry{margin=0in}
        \eject \pdfpagewidth=\CoverWidth \pdfpageheight=\CoverHeight
        \begin{figure}
            \input{ #1 }
        \end{figure}
        \restoregeometry
    }
}

